@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 20px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-meiyoushuju:before {
  content: "\e60b";
}

.no-data-box{
	position: fixed;
	display: flex;
	background: #f8f8f9;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 1000;
}
.no-data-box-fix{
	position: absolute;
}
.no-data-box-item{
	text-align: center;
	margin: auto;
	color: #1c2438;
}
.no-data-box-item-icon{
	font-size: 60px;
	color: #80848f;
}
.no-data-box-item-txt, .no-data-errTxt{
	font-size: 16px;
	text-align: center;
}
.no-data-errTxt{
	padding-top: 2px;
	font-size: 16px;
	color: #80848f;
}
.no-data-btn{
	font-size: 16px !important;
	margin-top: 6px;
	color: #495060;
	background-color: #fff;
	border: 1px solid #e9eaec;
}
.no-data-btn::after{
	border: none;
}
.no-data-btn:active{
	background-color: #e9eaec;
}
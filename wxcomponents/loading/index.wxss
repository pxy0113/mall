.loading-box{
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 10000;
	display: flex;
}
.loading-item{
	padding: 20px 28px 14px;
	margin: auto;
	text-align: center;
	border-radius: 4px;
	background: rgba(0,0,0,.7);
}
.loading-text{
	margin-top: 14px;
	color: #fff;
	font-size: 14px;
}
.loading-icon{
	display: inline-block;
	text-align: center;
	vertical-align: middle;
}
/*加载动画*/
.logoIcon{
	width: 25px;
	height: 25px;
	background: transparent url(data:image/png;base64,) no-repeat;
	background-size: 100%;
	animation: logoIcon 0.6s linear;
	animation-iteration-count: infinite;
}
@keyframes logoIcon{
	0% {
		transform: rotate(0);
	}
	100% {
		transform: rotate(360deg);
	}
}

.primalIcon{
	width: 30px;
	height: 30px;
	background: transparent url(data:image/svg+xml;base64,) no-repeat;
	background-size: 100%;
	animation: primalIcon 1s steps(12,end) infinite;
}
@keyframes primalIcon{
  0% {
    transform: rotate3d(0,0,1,0deg)
  }
  100% {
    transform: rotate3d(0,0,1,360deg)
  }
}